# mount point	fstype    device    	[device2]

/boot      /dev/block/mmcblk0p16 emmc
/cache     /dev/block/mmcblk0p25 ext4
/data      /dev/block/mmcblk0p27 ext4
/recovery  /dev/block/mmcblk0p17 emmc
/sdcard	   /dev/null datamedia
/system     dev/block/mmcblk0p24 ext4
/external_sd  /dev/block/mmcblk1p1 vfat
/preload    /dev/block/mmcblk0p26 ext4

